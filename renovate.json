{
  "extends": [
    "config:recommended",
    ":preserveSemverRanges",
    ":rebaseStalePrs"
  ],
  "baseBranchPatterns": [
    "main"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "digest"
      ],
      "automerge": false,
      "groupName": "all non-major dependencies",
      "groupSlug": "all-minor-patch",
      "matchPackageNames": [
        "*"
      ]
    }
  ]
}
